unit FDataFind;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.Grids, Vcl.StdCtrls;

type
  TFindForm = class(TForm)
    FindSG: TStringGrid;
    Label1: TLabel;
    Label2: TLabel;
    ComboBox1: TComboBox;
    ComboBox2: TComboBox;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FindForm: TFindForm;

implementation

{$R *.dfm}

procedure TFindForm.FormCreate(Sender: TObject);
begin
FindSG.Cells[0,0]:=('Номер');
FindSG.Cells[1,0]:=('Тип');
FindSG.Cells[2,0]:=('Наименование');
FindSG.Cells[3,0]:=('Кол-во з/е');
FindSG.Cells[4,0]:=('Оценка');
FindSG.Cells[5,0]:=('Дата');
FindSG.Cells[6,0]:=('Преподаватель');
FindSG.Cells[7,0]:=('Семестр');
end;

end.
